    "import cv2 \n",
    "  \n",
    "# Read the image. \n",
    "img = cv2.imread('D:/IRDE/frame.jpg') \n",
    "  \n",
    "# Apply bilateral filter with d = 15,  \n",
    "# sigmaColor = sigmaSpace = 75. \n",
    "bilateral = cv2.bilateralFilter(img, 20, 125, 125) \n",
    "  \n",
    "# Save the output. \n",
    "cv2.imwrite('D:/IRDE/frame_bilateral.jpg', bilateral) "
   ]
