#include<bits/stdc++.h> 
using namespace std; 
  
// Function to rightRotate array 
void RightRotate(int a[], int n, int k) 
{ 
      
    // If rotation is greater  
    // than size of array 
    k = k % n; 
  
    for(int i = 0; i < n; i++) 
    { 
       if(i < k) 
       { 
             
           // Printing rightmost  
           // kth elements 
           cout << a[n + i - k] << " "; 
       } 
       else
       { 
             
           // Prints array after 
           // 'k' elements 
           cout << (a[i - k]) << " "; 
       } 
    } 
    cout << "\n"; 
} 

// Function to leftRotate array multiple times
void leftRotate(int arr[], int n, int k)
{
    /* To get the starting point of rotated array */
    int mod = k % n;
 
    // Prints the rotated array from start position
    for (int i = 0; i < n; i++)
        cout << (arr[(mod + i) % n]) << " ";
 
    cout << "\n";
}
      
// Driver code 
int main() 
{ 
    int n; 
	cin>>n;
	int a[n]={};	
	for(int i=0;i<n;i++)
	{
		cin>>a[i];
	}
	
	int k;
    cin>>k;   
    RightRotate(a, n, k);
    leftRotate(a, n, k);

return 0;
} 
